# Grist Payment Updater Environment Variables
# Copy this file to .env and fill in your values for standalone use

# Your Grist API key (required)
GRIST_API_KEY=your-grist-api-key-here

# Proxy authorization header for Authelia (required)
GRIST_PROXY_AUTH=Basic your-proxy-auth-here

# Set to false to make actual changes (default: true for safety)
DRY_RUN=true